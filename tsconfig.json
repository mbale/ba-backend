{
  "compilerOptions": {
     "lib": [
        "es5",
        "es6",
        "es2017"
     ],
     "allowJs": true,
     "target": "es2016",
     "module": "commonjs",
     "moduleResolution": "node",
     "sourceMap": true,
     "noImplicitAny": false,
     "suppressImplicitAnyIndexErrors": true,
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true
  }
}